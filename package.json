{
  "name": "llantatech",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/llantatech-server/main.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docs": "npx compodoc -p tsconfig.docs.json -d docs/compodoc",
    "docs:serve": "npx compodoc -s -d docs/compodoc",
    "build:ssr": "ng build --configuration production --output-hashing=none && ng run llantatech:server:production",
    "serve:ssr": "node dist/llantatech-server/main.js",
    "dev:ssr": "ng run llantatech:serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^17.3.0",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/platform-server": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@angular/ssr": "^18.2.19",
    "@fullcalendar/angular": "^6.1.10",
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@iplab/ngx-file-upload": "^17.0.0",
    "apexcharts": "^3.54.1",
    "express": "^4.18.2",
    "moment": "^2.30.1",
    "ng-apexcharts": "~1.10.0",
    "ngx-color-picker": "^16.0.0",
    "ngx-editor": "^16.0.1",
    "ngx-gauge": "^9.0.0",
    "ngx-mat-timepicker": "^17.0.3",
    "ngx-owl-carousel-o": "^17.0.0",
    "ngx-scrollbar": "^13.0.3",
    "remixicon": "^4.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.19",
    "@angular/cli": "^18.2.19",
    "@angular/compiler-cli": "^18.2.13",
    "@angular/language-service": "^18.2.13",
    "@compodoc/compodoc": "^1.1.26",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
